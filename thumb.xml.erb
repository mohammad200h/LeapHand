<%
    require 'erb'
    require 'matrix'
    require_relative "thumb"

    thumb = Thumb.new()
%>

<mujoco>
  <compiler angle="radian" meshdir="assets"  autolimits="true"/>

  <asset>
    <%=thumb.assets_xml%>
  </asset>

  <default>
    <%=thumb.default_xml%>
  </default>

  <worldbody>
    <%=thumb.body_xml%>
  </worldbody>

  <actuator>
    <%=thumb.actuator_xml%>
  </actuator>

</mujoco>
